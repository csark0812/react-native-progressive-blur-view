		"@types/react-native": "^0.72.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.0.0",
		"jest": "^29.0.0",
		"react": "^18.2.0",
		"react-native": "^0.72.0",
		"react-native-builder-bob": "^0.20.0",
